class Solution {
    public int firstUniqChar(String s) {
        int[] ar = new int[26];
        for( int i = 0; i < s.length(); i++)
        {
            char tmp = s.charAt(i);
            ar[tmp - 'a'] = ++ar[tmp - 'a'];
        }
        for( int i = 0; i < s.length(); i++)
        {
            char tmp = s.charAt(i);
            if(ar[tmp - 'a'] == 1)
            {
                return i;
            }
        }
        return -1;
    }
    
}
