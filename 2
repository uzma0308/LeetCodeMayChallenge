class Solution {
    public int numJewelsInStones(String J, String S) {
        int count = 0;
        char[] myString = S.toCharArray(); 
        for (char c : myString) { 
            int contain= J.indexOf(c);
            if( contain != -1 )
            {
                count++;
            }
        } 
        return count;
    }
}
